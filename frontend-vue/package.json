{
  "name": "frontend-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky install frontend-vue/.husky",
    "predev": "node aspnetcore-https.js",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json --ignore-path .gitignore",
    "lint-fix": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json --ignore-path .gitignore --fix",
    "format": "prettier . --ignore-unknown --ignore-path .gitignore --check --write && npm run lint-fix"
  },
  "dependencies": {
    "@vueuse/core": "^7.6.2",
    "pinia": "^2.0.11",
    "swagger-client": "^3.18.4",
    "vite-plugin-eslint": "^1.3.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.3"
  }
}
