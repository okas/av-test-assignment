{
  "name": "frontend-vue",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "cd .. && husky install frontend-vue/.husky",
    "predev": "node aspnetcore-https.js",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint-script": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json --ignore-path .gitignore",
    "lint-style": "stylelint '**/*.{css,scss,sass,vue}'",
    "lint-script-fix": "eslint . --ext .ts,.tsx,.jsx,.cjs,.mjs,.js,.vue,.json --ignore-path .gitignore --fix",
    "lint-style-fix": "stylelint ./**/*.{css,scss,sass,vue} --fix",
    "format": "prettier . --ignore-unknown --ignore-path .gitignore --check --write && npm run lint-script-fix && npm run lint-style-fix"
  },
  "dependencies": {
    "@vueuse/core": "^7.6.2",
    "pinia": "^2.0.11",
    "swagger-client": "^3.18.4",
    "vite-plugin-eslint": "^1.3.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "sass": "^1.49.8",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.5",
    "vite": "^2.8.4"
  }
}
